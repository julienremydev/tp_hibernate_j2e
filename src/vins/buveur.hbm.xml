<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="vins.Buveur" table="buveur">
    <id column="NumeroBuv" name="numeroBuv">
      <generator class="increment"/>
    </id>
    <property name="nom"/>
    <property name="prenom"/>
    <set name="abu" table="ABU">
      <key column="numeroBuv"/>
      <many-to-many class="vins.Vin" column="Numero"/>
    </set>
  </class>
</hibernate-mapping>
