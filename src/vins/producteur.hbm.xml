<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="vins.Producteur" table="producteur">
    <id column="Numero" name="numero">
      <generator class="increment"/>
    </id>
    <property name="nom"/>
    <property name="adresse"/>
    <set name="lesVins" inverse="true" >
      <key column="ID_Prod"/>
      <one-to-many class="vins.Vin" />
    </set>
  </class>
</hibernate-mapping>
